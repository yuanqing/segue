{
  "name": "segue",
  "version": "0.0.2",
  "description": "Enqueue functions, and call them sequentially.",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/segue.git"
  },
  "devDependencies": {
    "istanbul": "^0.3.0",
    "jasmine-node": "^1.14.5",
    "jshint": "^2.5.4",
    "jshint-stylish": "^0.4.0"
  },
  "scripts": {
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/jasmine-node ./spec -x **/spec/**",
    "lint": "npm run lint-lib && npm run lint-test",
    "lint-lib": "node_modules/.bin/jshint --verbose --reporter node_modules/jshint-stylish/stylish.js *.js",
    "lint-test": "node_modules/.bin/jshint --verbose --reporter node_modules/jshint-stylish/stylish.js ./spec/*.js",
    "test": "node_modules/.bin/jasmine-node spec --verbose"
  },
  "keywords": [
    "arguments",
    "async",
    "asynchronous",
    "callback",
    "flow",
    "queue",
    "sequential",
    "serial",
    "series",
    "waterfall"
  ]
}
